zip_filename=$(date +%Yy%mm%dd_%Hh%Mm%Ss).aciss.zip
echo "Archiving previous run's log and data:"
zip -r ${zip_filename} ./log ./data
mv ${zip_filename} ./archive/.

echo "Clearing previous run's log and data:"
rm -rf ./log
rm -rf ./data
mkdir log
mkdir data
echo "Submitting the script:  [24.pbs]"
qsub -v SOS_CMD_PORT="22500" 24.pbs
sleep 3
qstat -u cdw
