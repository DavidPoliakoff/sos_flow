if ls $SOS_WORK/sosd.*.db 1> /dev/null 2>&1
then 
    for db in $SOS_WORK/sosd.*.db ; do
        echo -n "[${db}]: max(rowid)|avg(latency) = "
        sqlite3 ${db} "select max(rowid), avg(time_recv - time_send) from tblvals;"
    done
else
    echo ""
    echo "There are no database files to display in \$SOS_WORK ..."
    echo ""
    echo -n "    "
    env | grep -F SOS_WORK
    echo ""
fi
